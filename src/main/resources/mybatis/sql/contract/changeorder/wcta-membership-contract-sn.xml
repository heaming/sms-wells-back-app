<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kyowon.sms.wells.web.contract.changeorder.mapper.WctaMembershipContractSnMapper">
    <select id="selectMembershipContractSn" resultType="com.kyowon.sms.wells.web.contract.changeorder.dto.WctaMembershipContractSnDto$SearchRes">
        SELECT CNTR_SN
          FROM TB_SSCT_CNTR_DTL
         WHERE CNTR_NO = #{cntrNo}
           AND SELL_TP_CD  = #{sellTpCd}
    </select>
</mapper>
